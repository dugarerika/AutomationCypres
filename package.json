{
  "name": "cypress-automation-framework",
  "version": "1.0.0",
  "description": "Cypress Automation Framework",
  "main": "index.js",
  "scripts": {
    "cypress open": "./node_modules/.bin/cypress open",
    "delete:reportFolder": "rm -rf mochawesome-report/",
    "delete-mochawesome-separated-json-reports": "rm -rf cypress/reports/mochawesome-report/*.json || true",
    "delete-mochawesome-separated-html-reports": "rm -rf cypress/reports/mochawesome-report/*.html || true",
    "delete-mochawesome-report": "rm -rf mochawesome-report/* || true",
    "delete:mochawesome.json": "rm -rf mochawesome.json",
    "mochawesome-merge": "npx mochawesome-merge cypress/reports/mochawesome-report/*.json > mochawesome.json && npx marge mochawesome.json",
    "merge:reports": "mochawesome-merge cypress/reports/mochawesome-report/*.json > cypress-combined-report.json",
    "create:html:report": "npm run merge:reports && marge --reportDir TestReport cypress-combined-report.json",
    "headless": "npx cypress run",
    "headed": "npx cypress run --headed",
    "chrome": "npx cypress run --browser chrome",
    "dashboard": "npx cypress run --record --key 3025d595-10bc-4aad-91ed-5802e5896b45",
    "autoTestStore": "npx cypress run --spec cypress/e2e/automation-test-store/*",
    
    "OldVendor-Staging": "npx cypress run --reporter mochawesome --spec cypress/e2e/staging/*/*.js",
    "BetaVendor-Staging": "npx cypress run --reporter mochawesome --spec cypress/e2e/stagingBetaVendor/*/*.js",

    "OldVendor-Staging-Inventory-CreateProduct": "npx cypress run --spec cypress/e2e/stagingOldVendor/inventory/ov-stg-crtprod.cy.js",
    "BetaVendor-Staging-Inventory-CreateProduct": "npx cypress run --spec cypress/e2e/stagingBetaVendor/inventory/bv-stg-crtprod.cy.js",
    "BetaVendor-Staging-Inventory-DeleteProduct": "npx cypress run --spec cypress/e2e/stagingBetaVendor/inventory/bv-stg-dltprod.cy.js",
    "BetaVendor-Staging-Inventory-EditProduct": "npx cypress run --spec cypress/e2e/stagingBetaVendor/inventory/bv-stg-edtprod.cy.js",
    "BetaVendor-Staging-Inventory-DeleteSupplier": "npx cypress run --spec cypress/e2e/stagingBetaVendor/inventory/bv-stg-dltsup.cy.js",
    "BetaVendor-Staging-Inventory-DeleteCategory": "npx cypress run --spec cypress/e2e/stagingBetaVendor/inventory/bv-stg-dltscat.cy.js",
    
    "BetaVendor-Staging-Calendar-NewAppt": "npx cypress run --spec cypress/e2e/stagingBetaVendor/calendar/bv-stg-newappt.cy.js",
    "OldVendor-Staging-Calendar-CreateAppt": "npx cypress run --spec cypress/e2e/staging/calendar/ov-stg-crtapt.cy.js",
    "BetaVendor-Staging-Calendar-CreateAppt": "npx cypress run --spec cypress/e2e/stagingBetaVendor/calendar/bv-stg-crtapt.cy.js",
    "OldVendor-Staging-Calendar-BlockTime": "npx cypress run --spec cypress/e2e/staging/calendar/ov-stg-blcktime.cy.js",
    "BetaVendor-Staging-Calendar-BlockTime": "npx cypress run --spec cypress/e2e/stagingBetaVendor/calendar/bv-stg-blcktime.cy.js",
    "BetaVendor-Staging-Calendar-NewCheckout": "npx cypress run --spec cypress/e2e/stagingBetaVendor/calendar/bv-stg-newchck.cy.js",
    
    "OldVendor-Staging-Calendar-CreateEmployee": "npx cypress run --spec cypress/e2e/stagingBetaVendor/employees/bv-stg-crtemply.cy.js",
    "BetaVendor-Staging-Calendar-CreateEmployee": "npx cypress run --spec cypress/e2e/stagingBetaVendor/employees/bv-stg-crtemply.cy.js",
    "OldVendor-Production": "npx cypress run --spec cypress/e2e/production/calendar/ov-prod-crtapt.cy.js",
    "BetaVendor-Production": "npx cypress run --spec cypress/e2e/productionBetaVendor/calendar/bv-prod-blcktime.cy.js",
    
    "BetaVendor-Production-Calendar": "npx cypress run --reporter mochawesome --spec cypress/e2e/productionBetaVendor/calendar/*.js",
    "OldVendor-Production-Calendar": "npx cypress run --spec 'cypress/e2e/productionOldVendor/*.js",
    "OldVendor-Production-Calendar-CreateAppt": "npx cypress run --spec cypress/e2e/production/calendar/ov-prod-crtapt.cy.js",
    "BetaVendor-Production-Calendar-CreateAppt": "npx cypress run --spec cypress/e2e/productionBetaVendor/calendar/bv-prod-crtapt.cy.js",
    "BetaVendor-Production-Calendar-BlockTime": "npx cypress run --spec cypress/e2e/productionBetaVendor/calendar/bv-prod-blcktime.cy.js",
    "OldVendor-Production-Calendar-BlockTime": "npx cypress run --spec cypress/e2e/production/calendar/ov-prod-blcktime.cy.js",
    "BetaVendor-Production-Calendar-NewCheckout": "npx cypress run --spec cypress/e2e/productionBetaVendor/calendar/bv-prod-newchck.cy.js",
    
    "//": "PRODUCTION - INVENTORY",
    "BetaVendor-Production-Inventory": "npx cypress run --reporter mochawesome --spec cypress/e2e/productionBetaVendor/inventory/*.js",
    "Production-Inventory-CreateProduct-DeleteProduct": "npx cypress run --spec cypress/e2e/productionOldVendor/inventory/ov-prod-crtprod.cy.js cypress/e2e/production/inventory/bv-prod-crtprod.cy.js OldVendor-Production-Inventory-DeleteProduct",
    "OldVendor-Production-Inventory-CreateProduct": "npx cypress run --spec cypress/e2e/productionOldVendor/inventory/ov-prod-crtprod.cy.js",
    "BetaVendor-Production-Inventory-CreateProduct": "npx cypress run --spec cypress/e2e/productionBetaVendor/inventory/bv-prod-crtprod.cy.js",
    "OldVendor-Production-Inventory-DeleteProduct": "npx cypress run --spec cypress/e2e/productionOldVendor/inventory/ov-prod-dltprod.cy.js",
    "BetaVendor-Production-Inventory-DeleteProduct": "npx cypress run --spec cypress/e2e/productionBetaVendor/inventory/bv-prod-dltprod.cy.js",
    
    "BetaVendor-Staging-Employee-CreateEmployee": "npx cypress run --spec cypress/e2e/stagingBetaVendor/employees/bv-stg-crtemply.cy.js",
    "BetaVendor-Staging-Employee-DeleteEmployee": "npx cypress run --spec cypress/e2e/stagingBetaVendor/employees/bv-stg-dltemply.cy.js",
    "OldVendor-Staging-Employee-CreateStaff": "npx cypress run --spec cypress/e2e/stagingOldVendor/employees/ov-stg-staff.cy.js",
    "junit-merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/results.xml",
    "delete-junit-report": "rm -rf cypress/results/junit/results.xml",
    "delete-results": "rm -rf cypress/results/* || true",
    "cypress-regression-pack": "npm run delete-results && npm run delete-mochawesome-report && npm run headless && npm run mochawesome-merge",
    "staging": "npx cypress run --env configFile=staging",
    "cypress-multi-browser": "npm run autoTestStore -- --browser electron --headed & npm run autoTestStore -- --browser edge --headed"
  },
  "author": "Erika Tavera",
  "license": "ISC",
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "@faker-js/faker": "^9.0.1",
    "cypress": "^10.11.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "s^1.6.1",
    "cypress-xpath": "^2.0.1",
    "mocha": "^10.8.2",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.2.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/support/step_definitions"
  },
  "dependencies": {
    "-": "0.0.1",
    "save-dev": "0.0.1-security"
  }
}
