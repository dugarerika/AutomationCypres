{
	"name": "cypress-automation-framework",
	"version": "1.0.0",
	"description": "Cypress Automation Framework",
	"main": "index.js",
	"scripts": {
		"ui-regression": "npx cypress run --browser chrome --spec 'cypress/e2e/1-getting-started/**/*.js'",
		"ui-regression-allure": "npx cypress run --browser chrome --spec 'cypress/e2e/staging/inventory/*.cy.js'",
		"ui-regression-allure2": "npx cypress run --browser chrome --spec 'cypress/e2e/staging/customers/*.cy.js'",
		"allure:clear": "rm -rf allure-results/ allure-report cypress/screenshots || true",
		"allure:generate": "npx allure-commandline generate allure-results --clean -o allure-report",
		"allure:open": "npx allure-commandline open allure-report",
		"allure:report": "npx allure generate allure-results --clean -o allure-report",
		"allure:history": "mv -f allure-report/history allure-results/history && rm -rf allure-report || true",
		"cypress:open": "npx cypress open -b chrome",
		"delete:reportFolder": "rm -rf mochawesome-report/",
		"delete-mochawesome-separated-json-reports": "rm -rf cypress/reports/mochawesome-report/*.json || true",
		"delete-mochawesome-separated-html-reports": "rm -rf cypress/reports/mochawesome-report/*.html || true",
		"delete-mochawesome-report": "rm -rf mochawesome-report/* || true",
		"delete:mochawesome.json": "rm -rf mochawesome.json",
		"mochawesome-merge": "npx mochawesome-merge cypress/reports/mochawesome-report/*.json > mochawesome.json && npx marge mochawesome.json",
		"merge:reports": "npx mochawesome-merge cypress/reports/mochawesome-report/*.json > cypress-combined-report.json",
		"create:html:report": "npm run merge:reports && npx marge --reportDir TestReport cypress-combined-report.json",
		"headless": "npx cypress run --browser chrome",
		"headed": "npx cypress run --browser chrome --headed",
		"chrome": "npx cypress run --browser chrome",
		"dashboard": "npx cypress run --record --key 3025d595-10bc-4aad-91ed-5802e5896b45",
		"autoTestStore": "npx cypress run --browser chrome --spec 'cypress/e2e/automation-test-store/*'",
		"cy:prod-clear:cancelappts": "npx cypress run --spec 'cypress/e2e/production/clear/prod-cancelappts.cy.js'",
		"cy:prod-clear:dltprod": "npx cypress run --spec 'cypress/e2e/production/clear/prod-dltprod.cy.js'",
		"cy:prod-clear:dltsubs": "npx cypress run --spec 'cypress/e2e/production/clear/prod-dltsubs.cy.js'",
		"cy:prod-clear": "npx cypress run --spec 'cypress/e2e/production/clear/*.cy.js'",
		"cy:staging-clear": "npx cypress run --browser chrome --spec 'cypress/e2e/staging/clear/*.cy.js'",
		"cy:staging-clear:cancelappts": "npx cypress run --spec 'cypress/e2e/staging/clear/stg-cancelappts.cy.js'",
		"cy:staging-clear:dltemply": "npx cypress run --spec 'cypress/e2e/staging/clear/stg-dltemply.cy.js'",
		"cy:staging-clear:dltcustomers": "npx cypress run --spec 'cypress/e2e/staging/clear/stg-dltcust.cy.js'",
		"NAILEDIT-staging-clear:dltprod": "npx cypress run --spec 'cypress/e2e/staging/clear/stg-dltprod.cy.js'",
		"cy:staging-clear:dltbrand": "npx cypress run --spec 'cypress/e2e/staging/clear/stg-dltsbrand.cy.js'",
		"cy:staging-clear:dltscat": "npx cypress run --spec 'cypress/e2e/staging/clear/stg-dltscat.cy.js'",
		"cy:staging-clear:dltsubs": "npx cypress run --spec 'cypress/e2e/staging/clear/stg-dltsubs.cy.js'",
		"cy:staging-clear:dltsup": "npx cypress run --spec 'cypress/e2e/staging/clear/stg-dltsup.cy.js'",
		"cy:all-except-newcheckout": "npx cypress run --spec 'cypress/e2e/staging/calendar/{stg-blcktime.cy.js,stg-crtapt.cy.js,stg-newAppt.cy.js,stg-invoices.cy.js},cypress/e2e/staging/{coupons,customers,employees,inventory,offers,subscriptions}/*.cy.js'",
		"cy:all-except-newchk-crtapt-crtcust": "npx cypress run --spec \"cypress/e2e/staging/calendar/stg-blcktime.cy.js,cypress/e2e/staging/calendar/stg-invoices.cy.js,cypress/e2e/staging/calendar/stg-newAppt.cy.js,cypress/e2e/staging/coupons/stg-crtCoupons.cy.js,cypress/e2e/staging/employees/stg-attendances.cy.js,cypress/e2e/staging/employees/stg-contracts.cy.js,cypress/e2e/staging/employees/stg-crtemplcy.cy.js,cypress/e2e/staging/employees/stg-payroll.cy.js,cypress/e2e/staging/employees/stg-wrknghours.cy.js,cypress/e2e/staging/inventory/stg-crtprod.cy.js,cypress/e2e/staging/inventory/stg-crtsup.cy.js,cypress/e2e/staging/inventory/stg-edtsup.cy.js,cypress/e2e/staging/offers/stg-crtsoffer.cy.js,cypress/e2e/staging/subscriptions/stg-crtsubs.cy.js\"",
		"cy:staging:all": "npx cypress run --browser chrome --spec 'cypress/e2e/staging/**/*.cy.js'",
		"cy:prod:all": "npx cypress run --spec 'cypress/e2e/production/**/*.cy.js'",
		"cy:prod-inventory:createProducts": "npx cypress run --spec 'cypress/e2e/production/inventory/prod-crtprod.cy.js'",
		"cy:prod-inventory:edtsup": "npx cypress run --spec 'cypress/e2e/production/inventory/prod-edtsup.cy.js'",
		"cy:prod-calendar:newchck": "npx cypress run --spec 'cypress/e2e/production/calendar/prod-newchck.cy.js'",
		"cy:prod-calendar:aptchk": "npx cypress run --spec 'cypress/e2e/production/calendar/prod-apptchck.cy.js'",
		"cy:prod-calendar:blcktime": "npx cypress run --spec 'cypress/e2e/production/calendar/prod-blcktime.cy.js'",
		"cy:prod-calendar:crtapt": "npx cypress run --spec 'cypress/e2e/production/calendar/prod-crtapt.cy.js'",
		"cy:prod-calendar:invoices": "npx cypress run --spec 'cypress/e2e/production/calendar/prod-invoices.cy.js'",
		"cy:prod-calendar:newAppt": "npx cypress run --spec 'cypress/e2e/production/calendar/prod-invoices.cy.js'",
		"cy:coupons": "npx cypress run --spec 'cypress/e2e/staging/coupons/*.cy.js'",
		"cy:customers": "npx cypress run --spec 'cypress/e2e/staging/customers/*.cy.js'",
		"cy:employees": "npx cypress run --spec 'cypress/e2e/staging/employees/*.cy.js'",
		"cy:inventory": "npx cypress run --spec 'cypress/e2e/staging/inventory/*.cy.js'",
		"cy:offers": "npx cypress run --spec 'cypress/e2e/staging/offers/*.cy.js'",
		"cy:subscriptions": "npx cypress run --spec 'cypress/e2e/staging/subscriptions/*.cy.js'",
		"cy:staging-calendar:all": "npx cypress run -b chrome --spec 'cypress/e2e/staging/calendar/*.cy.js'",
		"cy:staging-calendar-inventory-employees": "npx cypress run --spec 'cypress/e2e/staging/calendar/*.cy.js,cypress/e2e/staging/inventory/*.cy.js,cypress/e2e/staging/employees/*.cy.js,'",
		"cy:staging-calendar:all-except-newchk-crtapt": "npx cypress run --spec 'cypress/e2e/staging/calendar/stg-blcktime.cy.js,cypress/e2e/staging/calendar/stg-invoices.cy.js,cypress/e2e/staging/calendar/stg-newAppt.cy.js'",
		"cy:staging-calendar:blcktime": "npx cypress run --spec 'cypress/e2e/staging/calendar/stg-blcktime.cy.js'",
		"ARTNAILCORNER-staging-calendar:crtapt": "npx cypress run --browser chrome --spec 'cypress/e2e/staging/calendar/stg-crtapt.cy.js'",
		"cy:staging-calendar:crtaptnaqa": "npx cypress run --browser chrome --spec 'cypress/e2e/staging/naqa/stg-crtaptfornaqa.cy.js'",
		"cy:staging-calendar:invoices": "npx cypress run --spec 'cypress/e2e/staging/calendar/stg-invoices.cy.js'",
		"ARTNAILCORNER-staging-calendar:newappt": "npx cypress run --browser chrome --spec 'cypress/e2e/staging/calendar/stg-newappt.cy.js'",
		"COCOCUTSALON-staging-calendar:newchck": "npx cypress run --spec 'cypress/e2e/staging/calendar/stg-newchck.cy.js'",
		"cy:staging-calendar:apptchck": "npx cypress run --spec 'cypress/e2e/staging/calendar/stg-apptchck.cy.js'",
		"cy:staging-coupons:crtcoupons": "npx cypress run --spec 'cypress/e2e/staging/coupons/stg-crtCoupons.cy.js'",
		"cy:staging-customers:crtcust": "npx cypress run --browser chrome --spec 'cypress/e2e/staging/customers/stg-crtcust.cy.js'",
		"cy:staging-employees:all": "npx cypress run --spec 'cypress/e2e/staging/employees/*.cy.js'",
		"cy:staging-employees:attendances": "npx cypress run --spec 'cypress/e2e/staging/employees/stg-attendances.cy.js'",
		"cy:staging-employees:contracts": "npx cypress run --spec 'cypress/e2e/staging/employees/stg-contracts.cy.js'",
		"NAILEDIT:staging-employees:crtemply": "npx cypress run --spec 'cypress/e2e/staging/employees/stg-crtemply.cy.js'",
		"cy:staging-employees:payroll": "npx cypress run --spec 'cypress/e2e/staging/employees/stg-payroll.cy.js'",
		"cy:staging-employees:wrknghours": "npx cypress run --spec 'cypress/e2e/staging/employees/stg-wrknghours.cy.js'",
		"cy:staging-inventory:crtprod": "npx cypress run --spec 'cypress/e2e/staging/inventory/stg-crtprod.cy.js'",
		"cy:staging-inventory:crtsup": "npx cypress run --spec 'cypress/e2e/staging/inventory/stg-crtsup.cy.js'",
		"cy:staging-inventory:edtsup": "npx cypress run --spec 'cypress/e2e/staging/inventory/stg-edtsup.cy.js'",
		"cy:staging-offers:crtsoffer": "npx cypress run --spec 'cypress/e2e/staging/offers/stg-crtsoffer.cy.js'",
		"cy:staging-subscriptions:crtsubs": "npx cypress run --spec 'cypress/e2e/staging/subscriptions/stg-crtsubs.cy.js'",
		"junit-merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/results.xml",
		"delete-junit-report": "rm -rf cypress/results/junit/results.xml",
		"delete-results": "rm -rf cypress/results/* || true",
		"cypress-regression-pack": "npm run delete-results && npm run delete-mochawesome-report && npm run headless && npm run mochawesome-merge",
		"staging": "npx cypress run --browser chrome --env configFile=staging",
		"cypress-multi-browser": "npm run autoTestStore -- --browser electron --headed & npm run autoTestStore -- --browser edge --headed"
	},
	"author": "Erika Tavera",
	"license": "ISC",
	"devDependencies": {
		"@badeball/cypress-cucumber-preprocessor": "^24.0.0",
		"@bahmutov/cypress-esbuild-preprocessor": "^2.2.8",
		"@cypress/xpath": "^2.0.3",
		"@faker-js/faker": "^9.6.0",
		"@shelex/cypress-allure-plugin": "^2.41.2",
		"allure-cypress": "^3.2.2",
		"chai": "^6.2.2",
		"cypress": "^15.10.0",
		"cypress-mochawesome-reporter": "^3.8.2",
		"cypress-multi-reporters": "^1.6.4",
		"cypress-slack-reporter": "^1.5.4",
		"cypress-xpath": "^2.0.1",
		"esbuild": "^0.27.2",
		"mocha": "^10.8.2",
		"mocha-junit-reporter": "^2.0.2",
		"mochawesome": "^7.1.3",
		"mochawesome-merge": "^4.4.1",
		"mochawesome-report-generator": "^6.2.0"
	},
	"cypress-cucumber-preprocessor": {
		"nonGlobalStepDefinitions": false,
		"stepDefinitions": "cypress/support/step_definitions"
	},
	"dependencies": {}
}
