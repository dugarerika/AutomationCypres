{
  "name": "cypress-automation-framework",
  "version": "1.0.0",
  "description": "Cypress Automation Framework",
  "main": "index.js",
  "scripts": {
    "ui-regression": "npx cypress run --browser chrome --spec cypress/e2e/1-getting-started/**/*.js",
    "ui-regression-allure": "npx cypress run --browser chrome --spec cypress/e2e/stagingBetaVendor/inventory/*.cy.js --env allure=true",
    "ui-regression-allure2": "cypress run --browser chrome --spec cypress/e2e/stagingBetaVendor/customers/*.cy.js --env allure=true",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:generate": "npx allure-commandline generate allure-results --clean -o allure-report",
    "allure:open": "npx allure-commandline open allure-report",
    "allure:report": "npx allure generate allure-results --clean -o allure-report",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true",
    "cypress open": "npx cypress open",
    "delete:reportFolder": "rm -rf mochawesome-report/",
    "delete-mochawesome-separated-json-reports": "rm -rf cypress/reports/mochawesome-report/*.json || true",
    "delete-mochawesome-separated-html-reports": "rm -rf cypress/reports/mochawesome-report/*.html || true",
    "delete-mochawesome-report": "rm -rf mochawesome-report/* || true",
    "delete:mochawesome.json": "rm -rf mochawesome.json",
    "mochawesome-merge": "npx mochawesome-merge cypress/reports/mochawesome-report/*.json > mochawesome.json && npx marge mochawesome.json",
    "merge:reports": "mochawesome-merge cypress/reports/mochawesome-report/*.json > cypress-combined-report.json",
    "create:html:report": "npm run merge:reports && marge --reportDir TestReport cypress-combined-report.json",
    "headless": "npx cypress run",
    "headed": "npx cypress run --headed",
    "chrome": "npx cypress run --browser chrome",
    "dashboard": "npx cypress run --record --key 3025d595-10bc-4aad-91ed-5802e5896b45",
    "autoTestStore": "npx cypress run --spec cypress/e2e/automation-test-store/*",
    "cy:prod-clear:cancelappts": "cypress run --spec 'cypress/e2e/production/clear/bv-prod-cancelappts.cy.js'",
    "cy:prod-clear:dltprod": "npx cypress run --spec cypress/e2e/production/clear/bv-prod-dltprod.cy.js",
    "cy:prod-clear:dltsubs": "cypress run --spec 'cypress/e2e/production/clear/bv-prod-dltsubs.cy.js'",
    "cy:prod-clear": "npx cypress run --spec cypress/e2e/production/clear/*.cy.js",
    "cy:staging-clear": "npx cypress run --spec cypress/e2e/staging/clear/*.cy.js",
    "cy:staging-clear:cancelappts": "cypress run --spec 'cypress/e2e/staging/clear/bv-stg-cancelappts.cy.js'",
    "cy:staging-clear:dltemply": "cypress run --spec 'cypress/e2e/staging/clear/bv-stg-dltemply.cy.js' --env allure=true",
    "cy:staging-clear:dltcustomers": "npx cypress run --spec cypress/e2e/staging/clear/dltcust.cy.js",
    "cy:staging-clear:dltprod": "npx cypress run --spec cypress/e2e/staging/clear/dltprod.cy.js",
    "cy:staging-clear:dltbrand": "cypress run --spec 'cypress/e2e/staging/clear/bv-stg-dltsbrand.cy.js'",
    "cy:staging-clear:dltscat": "cypress run --spec 'cypress/e2e/staging/clear/bv-stg-dltscat.cy.js'",
    "cy:staging-clear:dltsubs": "cypress run --spec 'cypress/e2e/staging/clear/bv-stg-dltsubs.cy.js'",
    "cy:staging-clear:dltsup": "cypress run --spec 'cypress/e2e/staging/clear/bv-stg-dltsup.cy.js'",
    "cy:all-except-newcheckout": "cypress run --spec 'cypress/e2e/stagingBetaVendor/calendar/{bv-stg-blcktime.cy.js,bv-stg-crtapt.cy.js,bv-stg-newAppt.cy.js,bv-stg-invoices.cy.js},cypress/e2e/stagingBetaVendor/{coupons,customers,employees,inventory,offers,subscriptions}/*.cy.js'",
    "cy:all-except-newchk-crtapt-crtcust": "cypress run --spec \"cypress/e2e/stagingBetaVendor/calendar/bv-stg-blcktime.cy.js,cypress/e2e/stagingBetaVendor/calendar/bv-stg-invoices.cy.js,cypress/e2e/stagingBetaVendor/calendar/bv-stg-newAppt.cy.js,cypress/e2e/stagingBetaVendor/coupons/bv-stg-crtCoupons.cy.js,cypress/e2e/stagingBetaVendor/employees/bv-stg-attendances.cy.js,cypress/e2e/stagingBetaVendor/employees/bv-stg-contracts.cy.js,cypress/e2e/stagingBetaVendor/employees/bv-stg-crtemplcy.cy.js,cypress/e2e/stagingBetaVendor/employees/bv-stg-payroll.cy.js,cypress/e2e/stagingBetaVendor/employees/bv-stg-wrknghours.cy.js,cypress/e2e/stagingBetaVendor/inventory/bv-stg-crtprod.cy.js,cypress/e2e/stagingBetaVendor/inventory/bv-stg-crtsup.cy.js,cypress/e2e/stagingBetaVendor/inventory/bv-stg-edtsup.cy.js,cypress/e2e/stagingBetaVendor/offers/bv-stg-crtsoffer.cy.js,cypress/e2e/stagingBetaVendor/subscriptions/bv-stg-crtsubs.cy.js\" --env allure=true",
    "cy:staging:all": "cypress run --spec 'cypress/e2e/stagingBetaVendor/**/*.cy.js'",
    "cy:prod:all": "cypress run --spec 'cypress/e2e/productionBetaVendor/**/*.cy.js'",
    "cy:prod-inventory:crtprod": "cypress run --spec 'cypress/e2e/productionBetaVendor/inventory/bv-prod-crtprod.cy.js'",
    "cy:prod-inventory:edtsup": "cypress run --spec 'cypress/e2e/productionBetaVendor/inventory/bv-prod-edtsup.cy.js'",
    "cy:prod-calendar:newchck": "cypress run --spec 'cypress/e2e/productionBetaVendor/calendar/bv-prod-newchck.cy.js'",
    "cy:prod-calendar:aptchk": "cypress run --spec 'cypress/e2e/productionBetaVendor/calendar/bv-prod-apptchck.cy.js'",
    "cy:prod-calendar:blcktime": "cypress run --spec 'cypress/e2e/productionBetaVendor/calendar/bv-prod-blcktime.cy.js'",
    "cy:prod-calendar:crtapt": "cypress run --spec 'cypress/e2e/productionBetaVendor/calendar/bv-prod-crtapt.cy.js'",
    "cy:prod-calendar:invoices": "cypress run --spec 'cypress/e2e/productionBetaVendor/calendar/bv-prod-invoices.cy.js'",
    "cy:prod-calendar:newAppt": "cypress run --spec 'cypress/e2e/productionBetaVendor/calendar/bv-prod-invoices.cy.js'",
    "cy:calendar": "cypress run --spec 'cypress/e2e/stagingBetaVendor/calendar/*.cy.js' --env allure=true",
    "cy:coupons": "cypress run --spec 'cypress/e2e/stagingBetaVendor/coupons/*.cy.js' --env allure=true",
    "cy:customers": "cypress run --spec 'cypress/e2e/stagingBetaVendor/customers/*.cy.js'",
    "cy:employees": "cypress run --spec 'cypress/e2e/stagingBetaVendor/employees/*.cy.js' --env allure=true",
    "cy:inventory": "cypress run --spec 'cypress/e2e/stagingBetaVendor/inventory/*.cy.js' --env allure=true",
    "cy:offers": "cypress run --spec 'cypress/e2e/stagingBetaVendor/offers/*.cy.js' --env allure=true",
    "cy:subscriptions": "cypress run --spec 'cypress/e2e/stagingBetaVendor/subscriptions/*.cy.js'",
    "cy:staging-calendar:all-except-newchk-crtapt": "cypress run --spec 'cypress/e2e/stagingBetaVendor/calendar/bv-stg-blcktime.cy.js,cypress/e2e/stagingBetaVendor/calendar/bv-stg-invoices.cy.js,cypress/e2e/stagingBetaVendor/calendar/bv-stg-newAppt.cy.js' --env allure=true",
    "cy:staging-calendar:blcktime": "cypress run --spec 'cypress/e2e/stagingBetaVendor/calendar/bv-stg-blcktime.cy.js'",
    "cy:staging-calendar:crtapt": "cypress run --browser chrome --spec 'cypress/e2e/stagingBetaVendor/calendar/bv-stg-crtapt.cy.js' --env allure=true",
    "cy:staging-calendar:invoices": "cypress run --spec 'cypress/e2e/stagingBetaVendor/calendar/bv-stg-invoices.cy.js'",
    "cy:staging-calendar:newappt": "cypress run --spec 'cypress/e2e/stagingBetaVendor/calendar/bv-stg-newappt.cy.js'",
    "cy:staging-calendar:newchck": "cypress run --spec 'cypress/e2e/stagingBetaVendor/calendar/bv-stg-newchck.cy.js'",
    "cy:staging-calendar:apptchck": "cypress run --spec 'cypress/e2e/stagingBetaVendor/calendar/bv-stg-apptchck.cy.js'",
    "cy:coupons:crtcoupons": "cypress run --spec 'cypress/e2e/stagingBetaVendor/coupons/bv-stg-crtCoupons.cy.js' --env allure=true",
    "cy:customers:crtcust": "cypress run --browser chrome --spec 'cypress/e2e/stagingBetaVendor/customers/bv-stg-crtcust.cy.js' --env allure=true",
    "cy:employees:attendances": "cypress run --spec 'cypress/e2e/stagingBetaVendor/employees/bv-stg-attendances.cy.js'",
    "cy:employees:contracts": "cypress run --spec 'cypress/e2e/stagingBetaVendor/employees/bv-stg-contracts.cy.js'",
    "cy:employees:crtemply": "cypress run --spec 'cypress/e2e/stagingBetaVendor/employees/bv-stg-crtemply.cy.js'",
    "cy:employees:payroll": "cypress run --spec 'cypress/e2e/stagingBetaVendor/employees/bv-stg-payroll.cy.js'",
    "cy:employees:wrknghours": "cypress run --spec 'cypress/e2e/stagingBetaVendor/employees/bv-stg-wrknghours.cy.js'",
    "cy:inventory:crtprod": "cypress run --spec 'cypress/e2e/stagingBetaVendor/inventory/bv-stg-crtprod.cy.js'",
    "cy:inventory:crtsup": "cypress run --spec 'cypress/e2e/stagingBetaVendor/inventory/bv-stg-crtsup.cy.js'",
    "cy:inventory:edtsup": "cypress run --spec 'cypress/e2e/stagingBetaVendor/inventory/bv-stg-edtsup.cy.js'",
    "cy:offers:crtsoffer": "cypress run --spec 'cypress/e2e/stagingBetaVendor/offers/bv-stg-crtsoffer.cy.js'",
    "cy:subscriptions:crtsubs": "cypress run --spec 'cypress/e2e/stagingBetaVendor/subscriptions/bv-stg-crtsubs.cy.js'",
    "junit-merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/results.xml",
    "delete-junit-report": "rm -rf cypress/results/junit/results.xml",
    "delete-results": "rm -rf cypress/results/* || true",
    "cypress-regression-pack": "npm run delete-results && npm run delete-mochawesome-report && npm run headless && npm run mochawesome-merge",
    "staging": "npx cypress run --env configFile=staging",
    "cypress-multi-browser": "npm run autoTestStore -- --browser electron --headed & npm run autoTestStore -- --browser edge --headed"
  },
  "author": "Erika Tavera",
  "license": "ISC",
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "@faker-js/faker": "^9.6.0",
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-cypress": "^3.2.2",
    "cypress": "^14.4.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-slack-reporter": "^1.5.4",
    "cypress-xpath": "^2.0.1",
    "mocha": "^10.8.2",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.2.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/support/step_definitions"
  },
  "dependencies": {
    "-": "0.0.1",
    "save-dev": "0.0.1-security"
  }
}
